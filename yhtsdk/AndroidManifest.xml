<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yunhetong.sdk">

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application android:allowBackup="false">
        <activity
            android:name="com.yunhetong.sdk.fast.ContractDetailActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name="com.yunhetong.sdk.fast.YhtServiceActivity"
            android:configChanges="orientation|keyboardHidden|screenSize" />
        <activity
            android:name="com.yunhetong.sdk.fast.SignDetailActivity"
            android:configChanges="orientation|keyboardHidden|screenSize" />
        <activity
            android:name="com.yunhetong.sdk.fast.SignGeneratorActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:screenOrientation="landscape" />
    </application>

</manifest>